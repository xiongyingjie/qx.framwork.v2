@model Web.Areas.Contents.ViewModels.CCV_M

@{
    Layout = ViewData["Layout"].ToString();
}
<label>
    内容表名称:&nbsp;
    @Html.DropDownListFor(o => o.CTD_ID, Model.ColumnNameselectItems, new { @class = "form-control input-sm" })
</label>

<script>
    function Search() {

        if (t('内容表名称') == '请选择')
            alert('请选择内容表名称');
        else
            var Params = $('#CTD_ID').val() + ';' + t("CTD_ID");
        return Params;
    }
</script>
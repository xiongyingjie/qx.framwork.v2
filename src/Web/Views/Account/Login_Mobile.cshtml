@model Web.ViewModels.Login_M
@{
// Layout = ViewData["Layout"].ToString();
}

<div class="container">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="login-panel panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">大健康企业云管理平台-登录</h3>
                </div>
                <div class="panel-body">
                    <form role="form" method="POST">
                        <fieldset>
                            <div class="form-group">
                                @Html.TextBoxFor(o => o.UserId, new { @class = "form-control", placeholder = "账号", autofocus = "autofocus" })

                            </div>
                            <div class="form-group">
                                @Html.PasswordFor(o => o.UserPsw, new { @class = "form-control", placeholder = "密码", })
                            </div>
                            <div class="form-group">
                                @Html.ValidationSummary()
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input name="remember" type="checkbox" value="Remember Me">记住密码
                                </label>
                            </div>
                            <!-- Change this to a button or input when using this as a form -->
                            <button type="submit" class="btn btn-lg btn-success btn-block">登陆</button>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

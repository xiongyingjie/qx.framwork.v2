<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewData["Title"]</title>
    <!--css core-->
    @Html.Partial("_src_css_sb2")
    @RenderSection("css", required: false)
</head>
<body>
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <h2 style="padding-bottom:10px;color:rgb(99,99,99);">
                @ViewData["Title"] <button class="btn btn-primary btn-sm back" onclick="back()" style="float:right;"> 上一级</button>
            </h2>
        </div>

    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <!-- /.panel-heading -->
                <div class="panel-body">
                <div class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                <ul class="list-ul">
                    @RenderBody()
                </ul>
                    @Html.Partial("_Report")
                </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
    </div>
</div>
    <!--js core-->
    @Html.Partial("_src_js_sb2")
    @RenderSection("scripts", required: false)
</body>
</html>
